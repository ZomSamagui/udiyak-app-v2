{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "navigation/*": ["navigation/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "hooks/*": ["hooks/*"],
      "lib/*": ["lib/*"],
      "screens/*": ["screens/*"],
      "type/*": ["type/*"],
    }
  },
  "include": [
    "**/*",
    "@types/**/*",
    "navigation/**/*",
    "assets/**/*",
    "components/**/*",
    "hooks/**/*",
    "lib/**/*",
    "screens/**/*",
    "type/**/*"
  ]
}